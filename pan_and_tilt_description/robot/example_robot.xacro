            <joint name="pitch_joint">
                <hardwareInterface>EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="motor2">
                <hardwareInterface>EffortJointInterface</hardwareInterface>
                <mechanicalReduction>118.1</mechanicalReduction>
            </actuator>
        </transmission>        
	    
    	

    
