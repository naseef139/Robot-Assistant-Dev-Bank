<?xml version="1.0" ?>

<robot name="pan_and_tilt" xmlns:xacro="http://www.ros.org/wiki/xacro">

  

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>



  <!--
        RGB CAMERAS:
-->
        <link name="main_camera_link">
            <!-- Inertial for gazebo to take it into account, Put 1gr weight -->
            <inertial>
                <origin xyz="0 0 2.0" rpy="0 0 0"/>
                <mass value="${maincamera_mass}" />
                <box_inertia 
				m="${maincamera_mass}" 
				x="${maincamera_size_long}" 
				y="${maincamera_size_short}" 
				z="${maincamera_size_short}"/>
            </inertial>
    		<visual>
                <origin rpy="0.0 0 0" xyz="0.00312 0 0.11152"/>
                <geometry>
                    <box size="${maincamera_size_long} ${maincamera_size_short} ${maincamera_size_short}"/>
                </geometry>
            </visual>
    	</link>
    
        <gazebo reference="main_camera_link">
            <material>Gazebo/Green</material>
        </gazebo>
    
        <joint name="main_camera_joint" type="fixed">
            <parent link="face_link"/>
            <child link="main_camera_link"/>
            <origin xyz="$0 1.0 0" rpy="0 0 0"/>
        </joint>
    
        <!-- camera -->
      <gazebo reference="main_camera_link">
        <sensor type="camera" name="camera1">
          <update_rate>10.0</update_rate>
          <camera name="main_cam">
    
              <pose>0 0 0 0 0 0</pose>
    
            <horizontal_fov>1.3962634</horizontal_fov>
            <image>
              <width>400</width>
              <height>400</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.01</near>
              <far>10.0</far>
            </clip>
            <noise>
              <type>gaussian</type>
              <stddev>0.0</stddev>
            </noise>
          </camera>
          <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
            <alwaysOn>true</alwaysOn>
            <updateRate>0.0</updateRate>
            <cameraName>pan_and_tilt/main_cam</cameraName>
            <imageTopicName>image_raw</imageTopicName>
            <cameraInfoTopicName>camera_info</cameraInfoTopicName>
            <frameName>main_camera_link</frameName>
            <hackBaseline>0.07</hackBaseline>
            <distortionK1>0.0</distortionK1>
            <distortionK2>0.0</distortionK2>
            <distortionK3>0.0</distortionK3>
            <distortionT1>0.0</distortionT1>
            <distortionT2>0.0</distortionT2>
          </plugin>
        </sensor>
      </gazebo>
   <xacro:include filename="$(find pan_and_tilt_description)/robot/common_properties1.xacro" />




